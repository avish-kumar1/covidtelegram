            $('.myDatatable').DataTable({
                initComplete: function () {
                    $(this.api().table().container()).find('input').parent().wrap('<form>').parent().attr('autocomplete', 'off');
                },

            });
            var url = window.location.pathname;
            if (url != "/Dashboard.aspx") {
                $('li').removeClass("active");
                $('a[href="' + url + '"]').parent().addClass("active");
                $('a[href="' + url + '"]').addClass("active");
                $('a[href="' + url + '"]').parent().parents("li.nav-item").addClass("active");
            }
        });


        function CallSuccessMsg(title, text) {
            swal(title, text, "success");
        }
        function CallErrorMsg(title, text) {
            swal(title, text, "error");
        }
        function CallWarningMsg(title, text) {
            swal(title, text, "warning");
        }
        var objDel = { status: false, ele: null };
        function functionConfirmSubmit(me, title, ConfirmButtonText) {

            if (objDel.status) return true;
            swal({
                title: title,
                text: "",
                icon: "warning",
                buttons: {
                    cancel: {
                        text: "No, cancel!",
                        value: null,
                        visible: true,
                        className: "",
                        closeModal: true,
                    },
                    confirm: {
                        text: "Yes, delete it!",
                        value: true,
                        visible: true,
                        className: "",
                        closeModal: false
                    }
                }
            })
                .then((isConfirm) => {
                    if (isConfirm) {
                        objDel.status = true;
                        objDel.ele.click();
                    }
                    else {
                        objDel.status = false;
                    }
                });
            objDel.ele = me;
            return false;
        }

    </script>
